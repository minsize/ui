{
    "name": "test",
    "version": "v1",
    "type": "module",
    "scripts": {
        "start": "vite --config config/dev.config.ts",
        "preview": "vite preview --config config/prod.config.ts --port 18300",
        "build": "tsc --project config/tsconfig.json && vite build --config config/prod.config.ts",
        "bundle": "npx vite-bundle-visualizer -c ./config/prod.config.ts",
        "odr": "tsc --project config/tsconfig.json && vite build --config config/odr.config.ts",
        "test": "vitest --config config/vitest.config.ts",
        "lib": "tsc --project config/tsconfig-build.json && vite build --config config/lib.config.ts"
    },
    "devDependencies": {
        "@types/node": "^20.11.24",
        "autoprefixer": "^10.4.20",
        "glob": "^11.0.0",
        "jsdom": "^25.0.1",
        "postcss": "^8.4.47",
        "rollup": "^4.21.3",
        "solid-js": "^1.8.16",
        "terser": "^5.29.2",
        "typescript": "^5.4.4",
        "vite": "^5.4.5",
        "vite-plugin-singlefile": "0.13.5",
        "vite-plugin-solid": "^2.10.2",
        "vite-plugin-solid-svg": "^0.8.0",
        "vite-plugin-zip-pack": "^1.2.1",
        "vite-tsconfig-paths": "^4.3.2"
    },
    "dependencies": {
        "@apiteam/twa-bridge": "^7.1.14",
        "@minsize/utils": "^0.0.5",
        "@solid-primitives/scheduled": "^1.4.4",
        "@solidjs/testing-library": "^0.8.10",
        "@testing-library/jest-dom": "^6.6.3",
        "elum-state": "^0.8.4",
        "solid-devtools": "^0.30.1",
        "solid-testing-library": "^0.5.1",
        "vite-plugin-css-modules": "^0.0.1",
        "vite-plugin-dts": "^4.3.0",
        "vite-plugin-lib-inject-css": "^2.1.1",
        "vitest": "^2.1.4"
    }
